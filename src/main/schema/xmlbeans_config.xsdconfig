<!-- configure java package names -->
<xb:config xmlns:xb="http://www.bea.com/2002/09/xbean/config">

    <xb:namespace uri="urn:oasis:names:tc:SAML:2.0:assertion">
        <xb:package>xmlbeans.org.oasis.saml2.assertion</xb:package>
    </xb:namespace>
    <xb:namespace uri="urn:oasis:names:tc:SAML:2.0:protocol">
        <xb:package>xmlbeans.org.oasis.saml2.protocol</xb:package>
    </xb:namespace>
    <xb:namespace uri="urn:oasis:names:tc:SAML:2.0:ac">
        <xb:package>xmlbeans.org.oasis.saml2.authnctx</xb:package>
    </xb:namespace>
    <xb:namespace uri="urn:oasis:names:tc:SAML:2.0:metadata">
        <xb:package>xmlbeans.org.oasis.saml2.metadata</xb:package>
    </xb:namespace>
    <xb:namespace uri="urn:oasis:names:tc:SAML:metadata:ui">
        <xb:package>xmlbeans.org.oasis.saml2.metadata.extui</xb:package>
    </xb:namespace>
    <xb:namespace uri="urn:oasis:names:tc:SAML:metadata:attribute">
        <xb:package>xmlbeans.org.oasis.saml2.metadata.extattribute</xb:package>
    </xb:namespace>

    <xb:namespace uri="urn:oasis:names:tc:SAML:2.0:ac:classes:PasswordProtectedTransport">
        <xb:package>xmlbeans.org.oasis.saml2.authnctx.ac.ppt</xb:package>
    </xb:namespace>
    <xb:namespace uri="urn:oasis:names:tc:SAML:2.0:ac:classes:Password">
        <xb:package>xmlbeans.org.oasis.saml2.authnctx.ac.pword</xb:package>
    </xb:namespace>
    <xb:namespace uri="urn:oasis:names:tc:SAML:2.0:ac:classes:PreviousSession">
        <xb:package>xmlbeans.org.oasis.saml2.authnctx.ac.session</xb:package>
    </xb:namespace>
    <xb:namespace uri="urn:oasis:names:tc:SAML:2.0:ac:classes:TLSClient">
        <xb:package>xmlbeans.org.oasis.saml2.authnctx.ac.sslcert</xb:package>
    </xb:namespace>
    
    
    <xb:namespace uri="http://www.w3.org/2001/04/xmlenc#">
        <xb:package>xmlbeans.org.w3.x2001.x04.xmlenc</xb:package>
    </xb:namespace>
    <xb:namespace uri="http://www.w3.org/2000/09/xmldsig#">
        <xb:package>xmlbeans.org.w3.x2000.x09.xmldsig</xb:package>
    </xb:namespace>
    
</xb:config>
